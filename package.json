{
  "name": "express-webpack-react-redux-typescript-boilerplate",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Armour/express-webpack-react-redux-typescript-boilerplate.git"
  },
  "author": "Chong Guo <armourcy@gmail.com>",
  "license": "MIT",
  "scripts": {
    "run-dev": "yarn build-dll && cross-env NODE_ENV=development yarn server",
    "run-prod": "yarn build-prod && cross-env NODE_ENV=production yarn server",
    "build-dll": "yarn clean && cross-env NODE_ENV=production webpack --config webpack.config.dll.babel.js",
    "build-prod": "yarn build-dll && cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "profile": "yarn build-dll && cross-env NODE_ENV=production webpack --config webpack.config.profile.babel.js",
    "server": "nodemon --exec babel-node backend/server.js",
    "eslint": "eslint -f codeframe \"**/*.js\" \"**/*.jsx\"",
    "tslint": "tslint -t codeFrame -e \"node_modules/**\" \"**/*.ts\" \"**/*.tsx\"",
    "stylelint": "stylelint \"**/*.css **/*.sass **/*.scss\"",
    "clean": "rm -rf frontend/dist .awcache coverage",
    "test": "jest --no-cache --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "deploy": "heroku container:push web",
    "precommit": "yarn eslint && yarn tslint && yarn stylelint",
    "prepush": "yarn test && yarn coverage"
  },
  "dependencies": {
    "@types/jest": "22.2.3",
    "@types/materialize-css": "1.0.3",
    "@types/prismjs": "1.9.0",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "5.0.19",
    "@types/react-router-dom": "4.2.6",
    "@types/redux-devtools": "3.0.44",
    "@types/redux-devtools-dock-monitor": "1.1.32",
    "@types/redux-devtools-log-monitor": "1.0.33",
    "@types/redux-logger": "3.0.6",
    "@types/superagent": "3.8.0",
    "@types/uuid": "3.4.3",
    "@types/webpack-env": "1.13.6",
    "add-asset-html-webpack-plugin": "2.1.3",
    "autoprefixer": "8.5.1",
    "awesome-typescript-loader": "5.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "body-parser": "1.18.3",
    "colors": "1.3.0",
    "compression": "1.7.2",
    "connect-redis": "3.3.3",
    "connected-react-router": "4.3.0",
    "cookie-parser": "1.4.3",
    "coveralls": "3.0.1",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "file-loader": "1.1.11",
    "helmet": "3.12.1",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.3.0",
    "immutable": "4.0.0-rc.9",
    "jest": "23.0.0",
    "material-design-icons": "3.0.1",
    "materialize-css": "1.0.0-rc.1",
    "mini-css-extract-plugin": "0.4.0",
    "morgan": "1.9.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "offline-plugin": "5.0.3",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "pg": "7.4.3",
    "postcss": "6.0.22",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.5",
    "prismjs": "1.14.0",
    "progress-bar-webpack-plugin": "1.11.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-hot-loader": "4.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
