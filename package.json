    "superagent": "3.8.3",
    "ts-jest": "22.4.6",
    "ts-loader": "4.3.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.3",
    "uglify-js": "3.3.27",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "uuid": "3.2.1",
    "webpack": "4.9.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "2.1.4",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.2",
    "webpack-pwa-manifest": "3.6.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "nodemonConfig": {
    "quiet": true,
    "watch": [
      "backend/controllers/",
      "backend/routes/",
      "backend/db/",
      "backend/config.json",
      "backend/server.js"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "tests"
    ],
    "moduleDirectories": [
      "node_modules",
      "frontend/src",
      "backend"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  }
}
