    "stylelint": "9.1.3",
    "stylelint-config-standard": "18.2.0",
    "superagent": "3.8.2",
    "ts-jest": "22.4.2",
    "ts-loader": "4.1.0",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "uglify-js": "3.3.16",
    "uglifyjs-webpack-plugin": "1.2.4",
